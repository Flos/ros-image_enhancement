<launch>
 <node pkg="nodelet" type="nodelet" name="image_enhancement_4clahe" args="load image_cloud/Clahe image_and_pcl_manager" output="screen">
  	<param name="sub" type="string" value="/ladybug5/camera4/raw_rect" />
  	<param name="pub" type="string" value="/ladybug5/camera4/raw_rect_clahe" />
  	<param name="cliplimit"  type="double" value="4" />
  </node>
  
  <node pkg="nodelet" type="nodelet" name="image_enhancement_4gamma" args="load image_cloud/Gamma image_and_pcl_manager" output="screen">
  	<param name="sub" type="string" value="/ladybug5/camera4/raw_rect" />
  	<param name="pub" type="string" value="/ladybug5/camera4/raw_rect_gamma" />
  	<param name="gamma"  type="double" value="2.2" />
  </node>
  
  <node pkg="nodelet" type="nodelet" name="image_enhancement_4brightness" args="load image_cloud/Brightness image_and_pcl_manager" output="screen">
  	<param name="sub" type="string" value="/ladybug5/camera4/raw_rect" />
  	<param name="pub" type="string" value="/ladybug5/camera4/raw_rect_brightness" />
  	<param name="alpha"  type="double" value="4" />
  	<param name="beta"   type="double" value="5" />
  </node>  
</launch>